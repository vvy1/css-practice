{{#with data}}
  <div class='card'>
    <div
      class='card_image'
      style='background-image: url({{photo}})'
    ></div>
    <h2 class='card_heading'>{{heading}}</h2>
    <p class='card_info'>{{info}}</p>
    <a
      href='{{linkUrl}}'
      class='card_link_text'
    >{{linkText}}</a>
  </div>
{{/with}}